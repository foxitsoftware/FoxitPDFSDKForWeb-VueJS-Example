{
  "name": "foxit-pdf-sdk-for-web-vuejs-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "npm install",
    "postsetup": "npm run update-sdk",
    "update-sdk": "node development/setup.js",
    "serve": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=development vue-cli-service serve",
